{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUnusedLocals" : true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "noUnusedParameters": false,
    "outDir": "./dist/",
    "sourceMap": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "lib": ["dom", "ES2017"],
    "module": "ES2015",
    "target": "es5",
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "functions"
  ]
}
